{
  "compilerOptions": {
  
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "service/*": ["service/*"],
      "assets":["assets/*"],
      "component":["component/*"],
      "redux":["redux/*"],
      "saga":["saga/*"],
      "pages":["pages/*"],
      "helpers":["helpers/*"],
      "data":["data/*"],
      "styles":["styles/*"],
      "utils":["utils/*"]
    },
    "types": [],
    "jsx": "react",     
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src", "vite.config.ts"]

  
}
